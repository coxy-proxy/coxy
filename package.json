  "scripts": {
    "build": "nx run-many --target=build --all",
    "build:frontend": "nx build frontend",
    "build:backend": "nx build backend",
    "build:gateway": "nx build gateway",
    "dev": "nx run-many --target=serve --all",
    "dev:frontend": "nx serve frontend",
    "dev:backend": "nx serve backend",
    "dev:gateway": "nx serve gateway",
    "test": "nx test backend",
    "e2e": "nx e2e gateway",
    "db:client": "prisma generate",
    "db:sync": "prisma db push",
    "ui:add": "TS_NODE_PROJECT=./tsconfig.base.json shadcn add",
    "lint": "biome check apps libs",
    "lint:staged": "biome check apps libs --staged",
    "init-env": "test -f .env || cp .env.default .env",
    "postinstall": "npm run init-env && prisma generate && prisma db push"
  },
  "bin": {
    "coxy": "./bin/cli.js"
  },
  "keywords": ["copilot", "proxy", "openai"],
  "files": [
    "dist/apps/backend",
    "dist/apps/gateway",
    "apps/frontend/server.js",
    "apps/frontend/.next",
